{
	"schemaVersion": 1,
	"id": "socket",
	"version": "${version}",
	"name": "Socket",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"froze"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/socket/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.socket.Socket"
		],
		"client": [
			"com.socket.SocketModClient"
		],
		"fabric-datagen": [
			"com.socket.SocketModDataGenerator"
		]
	},
	"mixins": [
		"socket.mixins.json",
		{
			"config": "modid.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}